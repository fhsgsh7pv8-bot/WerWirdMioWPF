<Page x:Class="WerWirdMioWPF.View.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WerWirdMioWPF.View" xmlns:viewmodel="clr-namespace:WerWirdMioWPF.ViewModel" d:DataContext="{d:DesignInstance Type=viewmodel:StartPageViewModel}"
      mc:Ignorable="d"
      d:DesignHeight="600" d:DesignWidth="920"
      Title="Start">

    <Grid>
        <TextBox HorizontalAlignment="Left" Margin="315,234,0,0" TextWrapping="Wrap" Text="{Binding UserName, UpdateSourceTrigger=PropertyChanged, Mode=TwoWay}" VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="325,292,0,0" VerticalAlignment="Top" Command="{Binding SetUserNameCommand}"/>

    </Grid>
</Page>
